.LTRMarqueeEffect {
    position: relative;
    width: 100%;
    height: 100%;
    overflow: visible;
    flex-direction: row-reverse;
    flex-grow: 0;
    flex-shrink: 1;
}

.RTLMarqueeEffect {
    position: relative;
    width: 100%;
    height: 100%;
    overflow: visible;
    flex-direction: row;
}

.MarqueeLText {
    font-size: 200px;
    white-space: nowrap;
    overflow: visible;
    -unity-text-align: upper-left;
    translate: 0 0;
    width: 100%;
    margin-right: 140px;
    margin-left: 140px;
    position: relative;
}

.MarqueeRText {
    font-size: 200px;
    white-space: nowrap;
    overflow: visible;
    -unity-text-align: upper-right;
    translate: 0 0;
    width: 100%;
    margin-left: 140px;
    margin-right: 140px;
}
