:root {
}

.Coroutine {
    flex-direction: column;
    width: 100%;
    height: 100%;
    flex-shrink: 0;
    position: absolute;
}

.UpperCoroutine {
    background-color: rgb(68, 69, 68);
    rotate: -10deg;
    scale: 2 2;
    transform-origin: 35% 300%;
    transition-duration: 3s;
    transition-timing-function: ease-out-bounce;
}

.ButtomCoroutine {
    background-color: rgb(68, 69, 68);
    rotate: -10deg;
    scale: 2 2;
    transform-origin: 65% -200%;
    transition-duration: 3s;
    transition-timing-function: ease-out-bounce;
}

.UpperCoroutineClose {
    background-color: rgb(68, 69, 68);
    rotate: -10deg;
    scale: 2 2;
    transform-origin: center 160%;
}

.ButtomCoroutineClose {
    background-color: rgb(68, 69, 68);
    rotate: -10deg;
    scale: 2 2;
    transform-origin: center -60%;
}

.Vignette {
    position: absolute;
    width: 100%;
    height: 100%;
    flex-shrink: 0;
    background-image: url("project://database/Assets/Textures/UI/Vignette.png?fileID=2800000&guid=a027b20287e697f4e81c5242ee285582&type=3#Vignette");
}

.LogoPosition {
    width: 100%;
    height: 100%;
    position: absolute;
}

.Logo {
    background-size: 100% 100%;
    background-image: url("project://database/Assets/Textures/UI/GameName.png?fileID=21300000&guid=c5958031809eb364d8e2b131ecd2e9bc&type=3#GameName");
    width: 30%;
    height: 30%;
    transform-origin: center 2000%;
    rotate: -10deg;
    position: absolute;
    top: 10%;
    left: 5%;
    border-left-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
}

.LogoToScreen {
    background-size: 100% 100%;
    background-image: url("project://database/Assets/Textures/UI/GameName.png?fileID=2800000&guid=c5958031809eb364d8e2b131ecd2e9bc&type=3#GameName");
    width: 30%;
    height: 30%;
    transform-origin: top;
    rotate: -10deg;
    position: absolute;
    top: 10%;
    left: 5%;
    border-top-color: rgba(0, 0, 0, 0);
    border-left-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
}

